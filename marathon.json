{
    "acceptedResourceRoles": ["*"],
    "cpus": 1,
    "mem": 32,
    "args": [
        "jupyterhub",
        "-f",
        "/srv/jupyterhub/jupyterhub_config.py"
    ],
    "container": {
        "docker": {
            "forcePullImage": true,
            "image": "r96941046/jupyterhubdcos",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 443,
                    "hostPort": 443,
                    "protocol": "tcp"
                }
            ]
        },
        "type": "DOCKER",
        "volumes": [
            {
                "containerPath": "mydata",
                "mode": "RW",
                "persistent": {
                    "type": "root",
                    "size": 1000
                }
            },
            {
                "containerPath": "/data",
                "hostPath": "mydata",
                "mode": "RW"
            }
        ]
    },
    "env": {
        "DOCKER_NETWORK_NAME": "BRIDGE",
        "DOCKER_NOTEBOOK_DIR": "/home/jovyan/work",
        "DOCKER_NOTEBOOK_IMAGE": "jupyter/scipy-notebook:2d878db5cbff",
        "DOCKER_SPAWN_CMD": "start-singleuser.sh",
        "GITHUB_CLIENT_ID": "abb9add51b36be7e4016",
        "GITHUB_CLIENT_SECRET": "65058166f576d5aca64c61aa88eb9a3c87c2736d",
        "OAUTH_CALLBACK_URL": "https://cloud365twagents.southeastasia.cloudapp.azure.com/hub/oauth_callback"
    },
    "fetch": [],
    "id": "hub",
    "requirePorts": true
}
